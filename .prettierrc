{
  "semi": false,
  "trailingComma": "all",
  "singleQuote": true,
  "tabWidth": 2,
  "quoteProps": "consistent",
  "printWidth": 120,
  "proseWrap": "preserve",
  "useTabs": false,
  "bracketSameLine": true,
  "endOfLine": "auto",
  "arrowParens": "avoid",
  "vueIndentScriptAndStyle": true,
  "singleAttributePerLine": false,
  "htmlWhitespaceSensitivity": "ignore"
}
